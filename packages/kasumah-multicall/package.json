    "ethereum-waffle": "^3.2.1",
    "hardhat": "^2.0.6",
    "hardhat-deploy": "^0.7.0-beta.39",
    "mocha": "^8.2.1",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
