    "local": "hardhat node",
    "start": "npm run local",
    "test": "hardhat test"
  },
  "bugs": {
    "url": "https://github.com/quorumcontrol/kasumah/issues"
  },
  "devDependencies": {
    "@gnosis.pm/safe-contracts": "^1.2.0",
